{
    "name": "abrouter/laravel-abtest",
    "type": "library",
    "description": "AbrLaravelClient :construction_worker_woman: is a PHP client for laravel to run ab-tests via ABRouter.",
    "license": "LGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Abrouter\\LaravelClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alex Yatsenko",
            "email": "abrouter@proxiedmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^5.3 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "abrouter/abrouter-php-client": "^0.22"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abrouter\\LaravelClient\\Providers\\AbrouterServiceProvider"
            ]
        }
    }
}
